{
    "name": "igorpecenikin/exprating",
    "license": "proprietary",
    "type": "project",
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:pivolan/SphinxsearchBundle.git"
        },
        {
            "type": "git",
            "url": "git@github.com:pivolan/EntityHiddenTypeBundle.git"
        },
        {
            "type": "git",
            "url": "git@github.com:pivolan/assetic.git"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.4.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "javiereguiluz/easyadmin-bundle": "^1.11",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "knplabs/knp-menu-bundle": "^2.0",
        "liip/imagine-bundle": "^1.4",
        "knplabs/knp-paginator-bundle": "^2.5",
        "stof/doctrine-extensions-bundle": "^1.2",
        "cocur/slugify": "^1.4",
        "egeloen/ckeditor-bundle": "^4.0",
        "symfony/assetic-bundle": "^2.7",
        "fzaninotto/faker": "^1.5",
        "oneup/uploader-bundle": "~1.5",
        "knplabs/knp-markdown-bundle": "~1.3",
        "iakumai/sphinxsearch-bundle": "0.2.6",
        "tetranz/select2entity-bundle": "^2.7",
        "glifery/entity-hidden-type-bundle": "^1.3",
        "emanueleminotto/twig-cache-bundle": "^1.0",
        "paquettg/php-html-parser": "^1.6"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^2.7",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "phpunit/phpunit": "^5.1"
    },
    "scripts": {
        "post-install-cmd": [
            "mkdir -p var/",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
