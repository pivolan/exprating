{% block breadcrumbs %}
    {{ parent() }}
    → <a href="{{ path("category_admin_categories") }}">Доступные категории</a>
{% endblock %}
{% block profile_body %}
    <div class="row">
        <div class="col-md-5">
            <h1>Категории</h1>
